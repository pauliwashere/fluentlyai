<div class="chat-window">
  <div class="bot-msg">
    <%= @bot.content %>
  </div>
</div>
<div class = "input-field">
  <%= simple_form_for [@conversation, @user_message] do |f| %>
      <%= f.input :content %>
      <%= f.submit %>
  <% end %>
</div>
<% @user_message.each do |item| %>
  item
<% end %>
